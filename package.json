{
  "name": "@tomjs/electron-devtools-files",
  "version": "0.0.0",
  "description": "Chrome devtools files for Electron",
  "keywords": [
    "node"
  ],
  "author": {
    "name": "Tom Gao",
    "email": "tom@tomgao.cc"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomgao365/electron-devtools-files.git"
  },
  "main": "./dist/index.js",
  "scripts": {
    "start": "tsx ./scripts/index.ts",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint \"scripts/**/*.ts\" *.{js,cjs,ts} --fix --cache",
    "lint:prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@tomjs/commitlint": "^2.0.5",
    "@tomjs/eslint": "^1.1.0",
    "@tomjs/prettier": "^1.0.6",
    "@tomjs/tsconfig": "^1.0.8",
    "@types/node": "^18.19.2",
    "@types/shelljs": "^0.8.15",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "shelljs": "^0.8.5",
    "tsx": "^4.6.2",
    "typescript": "~5.2.2"
  }
}
